#
# Define additional options used in moto releases
#
# dont touch ! really required, checked with kallsyms
CONFIG_DEBUG_KERNEL=y
CONFIG_DEBUG_MUTEXES=y
CONFIG_SCHED_DEBUG=y
CONFIG_OMAP_MBOX_FWK=y

# CPU Governors
# CONFIG_CPU_FREQ_DEFAULT_GOV_PERFORMANCE is not set
# CONFIG_CPU_FREQ_DEFAULT_GOV_POWERSAVE is not set
# CONFIG_CPU_FREQ_DEFAULT_GOV_USERSPACE is not set
CONFIG_CPU_FREQ_DEFAULT_GOV_ONDEMAND=y
# CONFIG_CPU_FREQ_DEFAULT_GOV_CONSERVATIVE is not set
CONFIG_CPU_FREQ_GOV_PERFORMANCE=y
CONFIG_CPU_FREQ_GOV_POWERSAVE=y
CONFIG_CPU_FREQ_GOV_USERSPACE=y
CONFIG_CPU_FREQ_GOV_BOOSTED=y
CONFIG_CPU_FREQ_GOV_SMARTASS=y

# SDCard optimisations
CONFIG_IOSCHED_SIO=y

# NTFS
CONFIG_NTFS_FS=y
# CONFIG_NTFS_DEBUG is not set
CONFIG_NTFS_RW=y

#
# ext4 kernel module
#
CONFIG_EXT4_FS=y
CONFIG_EXT4_FS_XATTR=y
# CONFIG_EXT4_FS_POSIX_ACL is not set
# CONFIG_EXT4_FS_SECURITY is not set
# CONFIG_EXT4_DEBUG is not set
CONFIG_JBD2=y

# NFS mount support
CONFIG_NFS_FS=y
CONFIG_NFS_V3=y
# CONFIG_NFS_V3_ACL is not set
# CONFIG_NFS_V4 is not set
# CONFIG_NFSD is not set
CONFIG_LOCKD=y
# CONFIG_LOCKD_V4 is not set
CONFIG_NFS_COMMON=y
CONFIG_SUNRPC=y
# CONFIG_RPCSEC_GSS_KRB5 is not set
# CONFIG_RPCSEC_GSS_SPKM3 is not set
CONFIG_CRYPTO_PCBC=y

# CIFS module
CONFIG_NETWORK_FILESYSTEMS=y
CONFIG_CIFS=m
# CONFIG_CIFS_STATS is not set
# CONFIG_CIFS_WEAK_PW_HASH is not set
# CONFIG_CIFS_UPCALL is not set
CONFIG_CIFS_XATTR=y
# CONFIG_CIFS_POSIX is not set
# CONFIG_CIFS_DEBUG2 is not set
CONFIG_CIFS_DFS_UPCALL=y
CONFIG_CIFS_EXPERIMENTAL=y
# CONFIG_NCP_FS is not set
# CONFIG_CODA_FS is not set
# CONFIG_AFS_FS is not set

CONFIG_NLS_UTF8=y

#
# Netfilters ipv4 and ipv6
#
CONFIG_NETFILTER_XT_TARGET_TCPMSS=y
CONFIG_NETFILTER_XT_MATCH_TCPMSS=y
CONFIG_IP_NF_MANGLE=y
CONFIG_IP_NF_RAW=y
CONFIG_IP6_NF_IPTABLES=y
CONFIG_IP6_NF_FILTER=y
CONFIG_IP6_NF_MANGLE=y
CONFIG_IP6_NF_RAW=y
CONFIG_IP6_NF_TARGET_REJECT=y
CONFIG_IP6_NF_TARGET_LOG=y
CONFIG_NETFILTER_XT_MATCH_MULTIPORT=y

# To build the SGX DDK
CONFIG_SGX_530_BUILD_RELEASE=y
CONFIG_SGX_REV125=y
CONFIG_SGX_CORE_REV=125

#
# 2ndboot compatible kernel
#
CONFIG_MAPPHONE_2NDBOOT=y
# CONFIG_EMU_UART_DEBUG is not set

CONFIG_SGX=y
CONFIG_PVR_OMAPLFB=y

CONFIG_IKCONFIG=y
CONFIG_IKCONFIG_PROC=y

# Other stuff
CONFIG_KERNEL_GZIP=y
CONFIG_ARM_THUMBEE=y

# Disabled
# CONFIG_WL127X_TEST is not set
