#
# Define additional options used in moto releases
#
# dont touch ! really required, checked with kallsyms
CONFIG_DEBUG_KERNEL=y
CONFIG_DEBUG_MUTEXES=y
CONFIG_SCHED_DEBUG=y
CONFIG_OMAP_MBOX_FWK=y

#
# ext4 kernel module
#
CONFIG_EXT4_FS=m
CONFIG_EXT4_FS_XATTR=y
# CONFIG_EXT4_FS_POSIX_ACL is not set
# CONFIG_EXT4_FS_SECURITY is not set
# CONFIG_EXT4_DEBUG is not set
CONFIG_JBD2=m

#
# Netfilters ipv4 and ipv6
#
CONFIG_NETFILTER_XT_TARGET_TCPMSS=m
CONFIG_NETFILTER_XT_MATCH_TCPMSS=m
CONFIG_IP_NF_MANGLE=m
CONFIG_IP_NF_RAW=m
CONFIG_IP6_NF_IPTABLES=m
CONFIG_IP6_NF_FILTER=m
CONFIG_IP6_NF_MANGLE=m
CONFIG_IP6_NF_RAW=m
CONFIG_IP6_NF_TARGET_REJECT=m
CONFIG_IP6_NF_TARGET_LOG=m

# To build the SGX DDK
CONFIG_SGX_530_BUILD_RELEASE=y
CONFIG_SGX_REV125=y
CONFIG_SGX_CORE_REV=125

#
# 2ndboot compatible kernel
#
CONFIG_MAPPHONE_2NDBOOT=y
CONFIG_SGX=y
CONFIG_PVR_OMAPLFB=y

